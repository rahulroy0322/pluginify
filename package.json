{
  "name": "pluginify",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A plugin system in react",
  "homepage": "https://github.com/rahulroy0322/pluginify#readme",
  "scripts": {
    "build": "npm run build:plugin&& npm run build:sw && npm run build:app",
    "build:app": "tsc -b && vite build",
    "build:sw": "cd public && tsc -p tsconfig.sw.json",
    "build:plugin": "cd public && tsc -p tsconfig.plugin.json",
    "check": "biome check",
    "check:fix": "biome check --fix",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-router": "^1.140.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-devtools": "^0.8.4",
    "@tanstack/react-router-devtools": "^1.140.0",
    "@tanstack/router-plugin": "^1.140.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulroy0322/pluginify.git"
  },
  "keywords": [
